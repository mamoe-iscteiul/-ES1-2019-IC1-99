<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.DetecaoErroController">
	<children>
		<Label contentDisplay="CENTER" text="Métodos com defeitos" />
		<TableView prefHeight="200.0" prefWidth="200.0">
			<columns>
				<TableColumn prefWidth="75.0" text="Method_id" />
				<TableColumn prefWidth="75.0" text="is_long_method" fx:id="is_long_column" />
				<TableColumn prefWidth="75.0" text="is_feature_envy" fx:id="is_envy_column"/>
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
		<Label text="Métodos sem defeitos" />
		<ListView prefHeight="200.0" prefWidth="200.0" />
	</children>
</VBox>
